<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="weatherAPIService" class="com.jwtexample.service.WeatherAPIService">
        <constructor-arg value="${api.weather.url}" />
    </bean>

    <bean id="tubeLineTimetableAPIService" class="com.jwtexample.service.TubeLineTimetableAPIService">
        <constructor-arg value="${api.tube.url}" />
    </bean>

</beans>